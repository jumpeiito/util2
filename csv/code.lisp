(defvar codehash
  (util::let1 h (make-hash-table :test #'equal)
	(util::for-each
	 (lambda (ll) (util::sethash (car ll) (cdr ll) h))
	 maincode)
	h))

(defun code->title (code)
  (gethash code codehash ""))

(defvar code-versa-hash
  (util::let1 h (make-hash-table :test #'equal)
	(util::for-each
	 (lambda (ll) (util::sethash (cdr ll) (car ll) h))
	 maincode)
	h))

(defun title->code (title)
  (gethash title code-versa-hash ""))

